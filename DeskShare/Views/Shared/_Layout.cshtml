<!DOCTYPE html>
<html lang="de">

<script>
    var Urls =
    {
        logout: '@Url.Action("LogOut", "Home")'
    }

</script>

<head>
    <meta charset="utf-8"/>
    <script src="~/lib/jquery/dist/jquery.js" type="text/javascript"></script>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - DeskShare</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css"/>


    <script src="~/lib/bootstrap/dist/js/bootstrap.js" type="text/javascript"></script>




</head>
<body>
<header>
    <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
        <div class="container">
            <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">DeskShare</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                <ul class="navbar-nav flex-grow-1">
                    <li class="nav-item">
                        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                    </li>

                </ul>
            </div>
            <button id="btnLogout" class="btn btn-primary" style="float: right">Logout</button>
        </div>
    </nav>
</header>
<div class="container">
    <main role="main" @*class="pb-3"*@style="background-color: antiquewhite">
        @RenderBody()
    </main>
</div>
</body>
</html>


